;
; BIND reverse data file for local 172.16.0.XXX net
;
$TTL    604800
@       IN      SOA     ns.l1m9.lan. root.l1m9.lan. (
                              1         ; Serial
                             8H         ; Refresh
                             2H         ; Retry
                             4W         ; Expire
                             1D )       ; Negative Cache TTL
;
@       IN      NS      ns.
77      IN      PTR     ns.l1m9.lan.
