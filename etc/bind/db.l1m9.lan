;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     l1m9.lan. root.l1m9.lan. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      l1m9.lan.
@       IN      A       172.16.0.77
@       IN      AAAA    ::1
